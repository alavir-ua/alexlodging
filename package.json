{
  "name": "alexlodging",
  "version": "1.0.0",
  "description": "Room booking web application built using the MERN stack",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "server": "nodemon server/index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node server/seeder",
    "data:destroy": "node server/seeder -d",
    "start": "node server/index.js",
    "docker:db": "docker-compose up -d"
  },
  "keywords": [
    "Three-Tier architecture",
    "SPA",
    "Room booking",
    "Node js",
    "React",
    "MongoDB",
    "Redux"
  ],
  "author": "Oleksii Fedorenko <alavir.web.dev@gmail.com>",
  "license": "MIT",
  "dependencies": {
		"@emotion/react": "^11.4.1",
		"@stripe/react-stripe-js": "^1.2.0",
		"@stripe/stripe-js": "^1.11.0",
		"aos": "^2.3.4",
		"axios": "^0.21.4",
		"bootstrap": "^5.1.1",
		"moment": "^2.29.1",
		"react": "^17.0.2",
		"react-bootstrap": "^1.6.3",
		"react-bootstrap-range-slider": "^3.0.2",
		"react-dom": "^17.0.2",
		"react-helmet": "^6.1.0",
		"react-redux": "^7.2.5",
		"react-router-bootstrap": "^0.25.0",
		"react-router-dom": "^5.3.0",
		"react-scripts": "4.0.3",
		"react-spinners": "^0.11.0",
		"redux": "^4.1.1",
		"redux-devtools-extension": "^2.13.9",
		"redux-thunk": "^2.3.0",
		"rsuite": "^5.0.1",
		"web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "aos": "^2.3.4",
    "axios": "^0.24.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.6.0",
    "faker": "^5.1.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-spinners": "^0.11.0"
  },
  "repository": {
    "type": "git",
